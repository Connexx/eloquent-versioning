{
    "name": "connexx/eloquent-versioning",
    "description": "An extension for the Eloquent ORM to support versioning.",
    "keywords": [
        "laravel",
        "orm",
        "eloquent",
        "revision",
        "version",
        "versioned",
        "versioning",
        "audit",
        "audits"
    ],
    "homepage": "https://github.com/Connexx/eloquent-versioning",
    "license": "MIT",
    "authors": [
        {
            "name": "Markus J. Wetzel",
            "email": "markuswetzel@gmx.net"
        },
        {
            "name": "Garret Gunter",
            "email": "garretgun@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "BinaryCocoa\\Versioning\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinaryCocoa\\Versioning\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
